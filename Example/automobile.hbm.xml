<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
           assembly="Example"
           namespace="Example">

  <class name="Automobile" table="[automobile]" lazy="true">

    <id name="Id_automobile" column="id_automobile" type ="int" >

      <generator class="identity" />
    </id>
    <property name="Description" column="description"  type ="string" />
    <property name="Id" column="id" type ="int"/>
    <property name="Registration_number" column="registration_number" type ="string"/>

  </class>
</hibernate-mapping>